@charset "utf-8";
@import url(//fonts.googleapis.com/earlyaccess/notosanskr.css);


/* common */
a, abbr, acronym, address, b, blockquote, big, body, center, caption, cite, code, command, dd, del, dfn, dl, div, dt, em, embed, fieldset, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html,
i, iframe, img, label, legend, li, object, ol, p, pre, q, s, samp, section, small, span, source, strike, strong, sub, sup, table, tbody, tfoot, thead, th, tr, u, ul, var{ margin:0; padding:0}
h1,h2,h3,h4,h5,h6{font-size:100% }
ol, ul{list-style:none}
address, caption, cite, code, dfn, em, strong, th, var{font-style :normal; font-weight:normal}
table{border-collapse:collapse; border-spacing:0}
fieldset, img{border:0}
img, input, button, select, textarea{ vertical-align:middle}
caption, th{text-align:left }
q:before, q:after{content :''} 
.hidden{width:0; height:0; margin:0; padding:0; overflow:hidden; line-height:0; font-size:0; visibility:hidden; position:absolute; left:0; top:0}


/* body */ 
body{width:100%; overflow-x:hidden; overflow-y:auto; transform:skew(-0.001deg); color:#333333; font-family: "Noto Sans KR", sans-serif !important;}
a, a:link, a:visited{text-decoration:none;}
a:hover, a:active{text-decoration:none;}

/* font */
.thin       { font-weight: 100 }
.light      { font-weight: 300 }
.regular    { font-weight: 400 }
.medium     { font-weight: 500 }
.bold       { font-weight: 700 }
.bolder     { font-weight: 900 }

.h10 {clear:both; height:10px;}
.h30 {clear:both; height:30px;}

h1 {font-size:42px; text-align:left; margin-bottom:60px; color:#333333;}
h2 {font-size:24px; text-align:left; margin-bottom:30px; color:#333333;}
h3 {font-size:18px; text-align:left; margin-bottom:30px; color:#333333; line-height:27px; }

.img01 {text-align:left; margin-bottom:60px;}


#wrap {width:100%;}
#wrap #top {width:100%; height:200px; text-align:center; margin:auto;}
#wrap #contents {width:100%;}
#wrap #contents .background {width:100%; padding:60px 0; background-color:#f0f2f5; margin:auto; text-align:center;}
#wrap #contents .background:after {clear:both; display:block;  content:"";}

#wrap #contents .background .colors {width:1080px; margin:auto; text-align:left;}
#wrap #contents .background .colors ul {width:100%; margin-bottom:60px;}
#wrap #contents .background .colors ul:after {clear:both; display:block;  content:"";}
#wrap #contents .background .colors ul li{width:160px; margin-right:24px; float:left;}
#wrap #contents .background .colors ul li:nth-child(6) {margin-right:0;}
#wrap #contents .background .colors ul li.txt {text-align:left; font-size:16px;color:#999999; line-height:24px; padding-top:25px;}
#wrap #contents .background .colors ul li p.palette01 {width:160px; height:120px; border-radius:10px; background-color:#b5d692}
#wrap #contents .background .colors ul li p.palette02 {width:160px; height:120px; border-radius:10px; background-color:#7ad5c3}
#wrap #contents .background .colors ul li p.palette03 {width:160px; height:120px; border-radius:10px; background-color:#333333}
#wrap #contents .background .colors ul li p.palette04 {width:160px; height:120px; border-radius:10px; background-color:#999999}
#wrap #contents .background .colors ul li p.palette05 {width:160px; height:120px; border-radius:10px; background-color:#c8cacc}
#wrap #contents .background .colors ul li p.palette06 {width:160px; height:120px; border-radius:10px; background-color:#f0f2f5; border:solid 1px #c8cacc;}
#wrap #contents .background .colors ul li p.palette07 {width:160px; height:120px; border-radius:10px; background-color:#f97d63}

#wrap #contents .background .typography {width:1080px; margin:auto; text-align:left;}
#wrap #contents .background .typography p.title {font-size:18px; margin-bottom:30px;}
#wrap #contents .background .typography .text01 {font-size:54px; color:#999999; line-height:81px; letter-spacing:-1px; margin-bottom:60px;}
#wrap #contents .background .typography .text02 {font-size:48px; color:#999999; line-height:72px; letter-spacing:-1px; margin-bottom:60px;}
#wrap #contents .background .typography .text03 {font-size:42px; color:#999999; line-height:63px; letter-spacing:-1px; margin-bottom:60px;}
#wrap #contents .background .typography .text04 {font-size:36px; color:#999999; line-height:54px; letter-spacing:-1px; }

#wrap #contents .background .margins, #wrap #contents .background .join, #wrap #contents .background .alert, #wrap #contents .background .login, #wrap #contents .background .profile, #wrap #contents .background .open_menu{width:1080px; margin:auto; text-align:left;}

